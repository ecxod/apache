{
    "name": "ecxod/apache",
    "description": "Langweilige Library des Ecxod Projekts",
    "type": "library",
    "keywords": ["ecxod", "apache"],
    "license": "MIT",
    "autoload": {
        "files": [
            "src/apache/Apache.php",
            "tests/ApacheTest.php"
        ],
        "psr-4": {
            "Ecxod\\Apache\\": "src/apache/",
            "Ecxod\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Christian Eichert",
            "email": "c@zp1.net"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0|^8.2",
        "predis/predis": "^2.3",
        "sentry/sdk": "^4.0"
    },
    "archive": {
        "exclude": [
            ".github",
            ".vscode",
            "tests",
            "vendor"
        ]
    },
    "suggests": {
        "phpunit/phpunit": "^11.5"
    }
}
