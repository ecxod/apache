{
    "name": "ecxod/apache",
    "description": "Langweilige Library des Ecxod Projekts",
    "type": "library",
    "keywords": [
        "ecxod",
        "apache",
        "php",
        "xml"
    ],
    "license": "MIT",
    "autoload": {
        "files": [
            "src/apache/Apache.php",
            "tests/apache/ApacheTest.php"
        ],
        "psr-4": {
            "Ecxod\\Apache\\": "src/apache/",
            "Ecxod\\Array2Xml\\": "src/array2xml/"
        }
    },
    "authors": [
        {
            "name": "Christian Eichert",
            "email": "c@zp1.net"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0|^8.2|^8.4",
        "predis/predis": "^2.3",
        "sentry/sdk": "^4.0",
        "ext-xmlwriter": "*"
    },
    "archive": {
        "exclude": [
            ".vscode",
            "vendor"
        ]
    }
}