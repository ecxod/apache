{
    "name": "ecxod/apache",
    "description": "Langweilige Library des Ecxod Projekts",
    "type": "library",
    "keywords": [
        "ecxod",
        "apache",
        "php",
        "xml"
    ],
    "license": "MIT",
    "autoload": {
        "files": [
            "src/apache/Apache.php",
            "tests/apache/ApacheTest.php"
        ],
        "psr-4": {
            "Ecxod\\Array2Xml\\": "src/array2xml/",
            "Ecxod\\Apache\\": "src/apache/",
            "Ecxod\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Christian Eichert",
            "email": "c@zp1.net"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0|^8.2",
        "predis/predis": "^2.3",
        "sentry/sdk": "^4.0",
        "ext-redis": "*",
        "ext-xmlwriter": "*"
    },
    "archive": {
        "exclude": [
            ".github",
            ".vscode",
            "tests",
            "vendor"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ecxod\\Tests\\Apache\\": "tests/apache/",
            "Ecxod\\Tests\\Array2Xml": "tests/array2xml/"
        }
    },
    "suggests": {
        "phpunit/phpunit": "^11.5"
    }
}